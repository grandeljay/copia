ration\" > \"<i>Produktstatus<\/i>\" > \"<i>Wenn Produktstatus inaktiv ist, wird der Lagerbestand wie 0 behandelt\" aktivieren<\/i><\/li>\n\t\t\t\t\t\t<\/ul>","key":"_#_platform_#_.quantity.type","type":"selection","values":{"stock":"Shop-Lagerbestand &uuml;bernehmen","stocksub":"Shop-Lagerbestand &uuml;bernehmen abzgl. Wert aus rechtem Feld","lump":"Pauschal (aus rechtem Feld)"},"default":"stock","morefields":{"quantity":{"key":"_#_platform_#_.quantity.value","type":"text","default":1,"verify":"int","cssClasses":["autoWidth"]}}},"mfn":{"label":"Alternativ-Hersteller","desc":"Falls ein Produkt keinen Hersteller hinterlegt hat, wird der hier angegebene Hersteller verwendet.","key":"_#_platform_#_.checkin.manufacturerfallback","type":"text"}}},"shipping":{"headline":"","desc":"<b>Versand<\/b>","fields":{"shippingtime":{"label":"Lieferzeit","key":"_#_platform_#_.shippingtime","type":"selection"},"shippingcost":{"label":"Versandkosten","key":"_#_platform_#_.shippingcost","type":"text"}}}}